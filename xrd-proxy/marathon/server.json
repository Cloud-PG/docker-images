{
    "id": "/xrootdproxy3",
    "cmd": null,
    "cpus": 12,
    "mem": 30000,
    "disk": 0,
    "instances": 1,
    "constraints": [
        [
        "hostname",
        "UNIQUE"
        ]
    ],
    "acceptedResourceRoles": [
        "*"
    ],
    "container": {
        "type": "DOCKER",
        "volumes": [
        {
            "containerPath": "/data/xrd",
            "hostPath": "/data/xrd",
            "mode": "RW"
        },
        {
            "containerPath": "/etc/grid-security/xrd",
            "hostPath": "/tmp",
            "mode": "RW"
        }
        ],
        "docker": {
        "image": "cloudpg/xrootd-proxy:xcache",
        "network": "HOST",
        "portMappings": [],
        "privileged": true,
        "parameters": [],
        "forcePullImage": true
        }
    },
    "env": {
        "CACHE_LOG_LEVEL": "debug",
        "REDIR_CMSD_PORT": "31376",
        "XrdSecGSISRVNAMES": "*",
        "REDIR_HOST": "10.0.0.117",
        "ORIGIN_HOST": "xrootd.ba.infn.it",
        "ORIGIN_XRD_PORT": "1094",
        "PROXY_CACHE": "10.0.0.236:8080"
    },
    "healthChecks": [
        {
        "gracePeriodSeconds": 300,
        "intervalSeconds": 60,
        "timeoutSeconds": 20,
        "maxConsecutiveFailures": 3,
        "delaySeconds": 15,
        "command": {
            "value": "curl -f -X GET http://localhost/check_health"
        },
        "protocol": "COMMAND"
        }
    ],
    "portDefinitions": [
        {
        "port": 10003,
        "protocol": "tcp",
        "labels": {}
        }
    ],
    "args": [
        "-P"
    ]
}
  